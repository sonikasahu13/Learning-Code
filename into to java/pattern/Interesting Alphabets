import java.util.Scanner;
public class Solution {
	public static void main(String[] args) {
        
		//Your code goes here
    { 
        int i, j;
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for (i = n; i >= 1; i--)  
        { 
            for (j = i; j <= n; j++) 
            { 
                System.out.printf("%c", ('A' + j - 1)); 
            } 
            System.out.println(); 
        } 
    } 
    }
}
