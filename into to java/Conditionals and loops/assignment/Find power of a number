import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int res=1;
		int n=sc.nextInt();
		int p=sc.nextInt();
		for(int i=1;i<=p;i++)
		{ 
			res=res*n;
		}
		System.out.println(res);
		
        }
	
    }
	
}
